{
  "name": "vue-vanilla-datetime-picker",
  "version": "1.0.18",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build-lib": "vue-cli-service build --target lib --name DateTimePicker ./src/components/DateTimePicker.vue",
    "lint": "vue-cli-service lint"
  },
  "author": "Vladyslav Shchepotin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Shchepotin/vue-vanilla-datetime-picker/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Shchepotin/vue-vanilla-datetime-picker.git"
  },
  "keywords": [
    "vue",
    "date",
    "datetime",
    "time",
    "picker"
  ],
  "main": "./dist/DateTimePicker.common.js",
  "dependencies": {
    "luxon": "^1.3.3",
    "vue": "^2.5.17",
    "vue-click-outside": "^1.0.7"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.0",
    "@vue/cli-plugin-eslint": "^3.0.0",
    "@vue/cli-service": "^3.0.0",
    "vue-template-compiler": "^2.5.17"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
